{
  "name": "ad-analysis-system",
  "version": "1.0.0",
  "description": "검색광고 경쟁력 분석 시스템",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": ["광고분석", "검색광고", "경쟁분석"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.20.1",
    "multer": "^1.4.5-lts.1",
    "tesseract.js": "^4.1.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 